    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "pg": "^8.6.0",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-destructuring": "^7.14.7",
    "@babel/plugin-transform-regenerator": "^7.13.15",
    "@babel/preset-env": "^7.14.2",
    "sequelize-cli": "^6.2.0"
